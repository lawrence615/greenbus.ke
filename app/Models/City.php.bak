<?php

namespace App\Models;

use Illuminate\Database\Eloquent\Model;
use Illuminate\Database\Eloquent\Relations\HasMany;
use App\Models\Tour;

class Location extends Model
{
    protected $fillable = [
        'name',
        'code',
        'slug',
        'country',
        'is_active',
    ];

    public function tours(): HasMany
    {
        return $this->hasMany(Tour::class);
    }
}
